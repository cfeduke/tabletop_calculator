= form_for @army_list do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :game_system_id
    %br
    = f.text_field :game_system_id
  %p
    = f.label :points
    %br
    = f.text_field :points
  %p
    = f.label :user_id
    %br
    = f.text_field :user_id
  %p
    = f.submit
